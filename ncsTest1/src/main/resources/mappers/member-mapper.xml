<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "mybatis-3-mapper.dtd" >
<mapper namespace="">

<!-- 	private String memberId;
	private String memberPwd;
	private String
	memeberName;
	private Date memberEnrollDate; -->
	<resultMap type="Member" id="memberResultSet">
		<id column="MEMBER_ID" property="memberId" />
		<result column="MEMBER_PWD" property="memberPwd" />
		<result column="MEMBER_NAME" property="memberName" />
		<result column="MEMBER_ENROLL_DATE" property="memberEnrollDate" />
	</resultMap>

	<select id="loginMember" resultMap="memberResultSet">
		select * from MEMBER where
		MEMBER_ID =#{memberId} and MEMBER_PWD=#{memberPwd}
	</select>
	<insert id="insertMemeber" parameterType="Member">
		INSERT INTO MEMBER VALUES(#{memberId},#{memberPwd},#{memberName}, DEFAULT);
	</insert>
</mapper>
